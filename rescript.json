{
  "name": "jvlk",
  "namespace": true,
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "@jvlk/rescript-classnames"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.tsx",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}